{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run migrate-reset && jest --coverage --forceExit --detectOpenHandles",
    "start": "node index.js",
    "undo-migrate": "sequelize-cli db:migrate:undo:all --env env",
    "migrate": "sequelize-cli db:migrate --env env",
    "migrate-reset": "npm run undo-migrate && npm run migrate"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./node_modules/",
      "./coverage/",
      "./database/migrations/",
      "./index.js"
    ],
    "collectCoverageFrom": [
      "./**/*.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  }
}
